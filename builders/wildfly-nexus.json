{
   "kind": "List",
   "apiVersion": "v1",
   "metadata": {
      "name": "wildfly-nexus"
   },
   "items": [
      {
         "kind": "BuildConfig",
         "apiVersion": "v1",
         "metadata": {
            "name": "wildfly-nexus"
         },
         "spec": {
            "triggers": [
               {
                  "type": "GitHub",
                  "github": {
                     "secret": "secret"
                  }
               },
               {
                  "type": "Generic",
                  "generic": {
                     "secret": "secret"
                  }
               },
               {
                  "type": "ImageChange",
                  "imageChange": {}
               },
               {
                  "type": "ConfigChange"
               }
            ],
            "source": {
               "type": "Git",
               "git": {
                  "uri": "https://github.com/jorgemoralespou/nexus-ose",
                  "ref": "master"
               },
               "contextDir": "builders/wildfly-nexus"
            },
            "strategy": {
               "type": "Docker",
               "dockerStrategy": {
                  "from": {
                     "kind": "DockerImage",
                     "name": "docker.io/openshift/wildfly-81-centos7:latest"
                  }
               }
            },
            "output": {
               "to": {
                  "kind": "ImageStreamTag",
                  "name": "wildfly-nexus:latest"
               }
            },
            "resources": {}
         }
      },
      {
         "kind": "ImageStream",
         "apiVersion": "v1",
         "metadata": {
            "name": "wildfly-nexus"
         },
         "spec": {
            "dockerImageRepository": "",
            "tags": [
               {
                  "name": "latest",
                  "annotations": {
                     "description": "Build and run Java applications on Wildfly 8.1 using internal Nexus repository",
                     "iconClass": "icon-java",
                     "supports": "java:1.8,wildfly:8.1",
                     "tags": "builder,wildfly-nexus",
                     "version": "8.1"
                  }
               }
            ]
         }
      }
   ]
}